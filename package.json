    "@types/inquirer": "^7.3.1",
    "@types/lodash": "^4.14.168",
    "archiver": "^2.0.0",
    "colors": "^1.4.0",
    "deep-object-diff": "^1.1.0",
    "git-ignore-parser": "0.0.2",
    "ignore": "^5.0.4",
    "js-yaml": "^4.1.0",
    "md5-file": "^5.0.0",
    "node-machine-id": "^1.1.12",
    "os-locale": "^5.0.0",
